<Window x:Class="ThreeDMineTools.ApplicationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ThreeDMineTools"
        mc:Ignorable="d"
        Title="ApplicationWindow" Height="450" Width="800">
    <Grid>
        <TabControl x:Name="Tabs">
            <TabItem>
                <TabItem.Header>Open new tab</TabItem.Header>
                <TabItem.Content>
                    <Grid Background="White">
                        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                            <Button Click="CreateVoxelPage">
                                <TextBlock Text="Model to schematic" FontSize="20"/>
                            </Button>
                        </StackPanel>
                    </Grid>
                </TabItem.Content>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
